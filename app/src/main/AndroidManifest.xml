    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AndModuleAds">
        <activity
            android:name=".admob.AdmobSimpleListActivity"
            android:exported="false" />
        <activity
            android:name=".applovin.MaxSimpleListActivity"
            android:exported="false" />
        <activity
            android:name=".applovin.MainApplovinActivity"
            android:exported="false" />
        <activity android:name=".admob.ContentActivity" />
        <activity android:name=".admob.MainActivity" />
        <activity android:name=".admob.SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="@string/admod_app_id" />
        <meta-data
            android:name="applovin.sdk.key"
